<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodels="clr-namespace:AssetManager.ViewModels;assembly=AssetManager.ViewModels"
                    xmlns:views="clr-namespace:AssetManager.Views">
    <DataTemplate DataType="{x:Type viewmodels:AssetViewModel}">
        <views:AssetView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AssetDetailsViewModel}">
        <views:AssetDetailsView />
    </DataTemplate>
</ResourceDictionary>
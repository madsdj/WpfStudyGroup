<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#F80"/>
    <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="#999" />
    <SolidColorBrush x:Key="Foreground" Color="#FFF" />
    <SolidColorBrush x:Key="Background" Color="#333" />
    <SolidColorBrush x:Key="HeaderBackground" Color="#444" />
    <SolidColorBrush x:Key="SettingsPanelBackground" Color="#555" />
    <SolidColorBrush x:Key="AssetListBackground" Color="#222" />

</ResourceDictionary>